<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
	<%= render 'layouts/stylesheets'%>
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"></script>
	<%= javascript_include_tag "application", "rails", 'swfobject' %>
	<%= favicon_link_tag %>
  </head>
  <body>
	<div class="container">
		<%= render 'layouts/header' %>
		<%= render 'layouts/separator' %>
		<section class="round">
			<%= yield %>
		</section>
	</div>
		<%= render 'layouts/footer' %>
  </body>
</html>
